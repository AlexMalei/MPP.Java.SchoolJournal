<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="school.journal.entity.Clazz" table="class" schema="school_journal_db">
        <id name="classId">
            <column name="class_id" sql-type="int(10) unsigned"/>
        </id>
        <property name="number">
            <column name="number" sql-type="int(11)"/>
        </property>
        <property name="letterMark">
            <column name="letter_mark" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="isDeleted">
            <column name="isDeleted" sql-type="tinyint(1)"/>
        </property>
    </class>
</hibernate-mapping>